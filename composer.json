{
    "type": "project",
    "license": "proprietary",
    "repositories": [{
        "type": "package",
        "package": {
            "name": "sachinchoolur/lightgallery",
            "version": "1.6.12",
            "dist": {
                "url": "https://github.com/sachinchoolur/lightGallery/archive/1.6.12.zip",
                "type": "zip"
            }
        }
   	}, {
        "type": "package",
        "package": {
            "name": "mateusz-matela/djvu-html5",
            "version": "0.2.2",
            "dist": {
                "url": "https://github.com/mateusz-matela/djvu-html5/releases/download/0.2.2/Djvu_html5-0.2.2.zip",
                "type": "zip",
                "reference": "master"
            }
        }
   	}],
    "require": {
        "php": ">=7.1.3",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "babdev/pagerfanta-bundle": ">=1.0",
        "chitanka/permission-bundle": "*",
        "chitanka/rocket-chat-client": "*",
        "chitanka/thumbnail-server": "*",
        "chitanka/utils": "*",
        "chitanka/wiki-bundle": "^1.1",
        "composer/package-versions-deprecated": "^1.11",
        "cosmicdreams/fontawesome-iconpicker": "dev-master",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.1",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "easycorp/easyadmin-bundle": "^3.0",
        "fortawesome/font-awesome": "v4.3.0",
        "friendsofsymfony/message-bundle": "^2.0",
        "mateusz-matela/djvu-html5": "^0.2.2",
        "phpdocumentor/reflection-docblock": "^5.2",
        "sachinchoolur/lightgallery": "*",
        "sensio/framework-extra-bundle": "^5.6",
        "stof/doctrine-extensions-bundle": "^1.2",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/intl": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.5",
        "symfony/process": "4.4.*",
        "symfony/property-access": "4.4.*",
        "symfony/property-info": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "^4.4",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/yaml": "4.4.*",
        "twig/extensions": "~1.0",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/string-extra": "^3.0",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^1.4"
    },
    "require-dev": {
        "symfony/browser-kit": "^4.4",
        "symfony/css-selector": "^4.4",
        "symfony/debug-bundle": "^4.4",
        "symfony/dotenv": "4.4.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^5.1",
        "symfony/stopwatch": "^4.4",
        "symfony/var-dumper": "^4.4",
        "symfony/web-profiler-bundle": "^4.4"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
